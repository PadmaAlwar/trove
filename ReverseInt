package JavaPrograms;
import java.util.*;
import java.lang.*;
import java.io.*;

public class ReverseInt {
 	public static void main(String ags[]){
	int n,r,rev=0;
	Scanner obj=new Scanner(System.in);
	try
	{
	n=obj.nextInt();
	while(n!=0)
	{
		r=n%10;
		rev=rev*10+r;
		n=n/10;
	}
	System.out.println(rev);
	}
	catch(InputMismatchException e)
	{
		System.out.println("Not A Number");
	}
	}
}
